.day-box .day-list li:first-child{border-radius: 6px 6px 0 0;}
.day-box .day-list li:last-child{border-radius: 0 0 6px 6px;}
.ask-page .day-list li .icon-m{
  font-size: 42px;
  color: #cbcbd0;
  margin-right: 10px;
}
.ask-page .day-list li.active .icon-m{color: #0d7aff;}
.sign-in .day-list li.active{
  background: #00afff;
  color: #fff;
}
.sign-in .day-list li .gold-count{
  height: 45px;
  line-height: 40px;
  float: right;
  color: #ff8020;
  background: url(../image/gold-bg.png) no-repeat 40px 0;
  padding-left: 95px;
  padding-right: 5px;
  margin-top: -5px;
}
.sign-in .day-list li.active .gold-count{color: #fff;}
.sign-in .day-list li .get-btn{
  float: right;
  width: 100px;
  height: 40px;
  line-height: 40px;
  color: #fff;
  background: #999;
  text-align: center;
  border-radius: 6px;
  font-size: 24px;
}

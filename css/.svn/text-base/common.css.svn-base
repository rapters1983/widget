  line-height: 80px;
  padding: 0 30px;
  border-bottom: 1px solid #cbcbcb;
}
.recharge .org{
  color: #f14500;
}
.recharge .til{
  height: 80px;
  padding: 35px 30px 0;
  color: #666;
  background: #eee;
  border-bottom: 1px solid #cbcbcb;
  font-size: 26px;
}
.recharge .limit-list{
  height: 100%;
  padding: 40px 30px 0;
}
.recharge .limit-list ul{
  width: 100%;
}
.recharge .limit-list ul li{
  width: 47%;
  height: 80px;
  padding: 15px 0 0 40px;
  background: #f6f7f8;
  border:1px solid #b7b7b7;
  float: left;
  border-radius: 6px;
  margin: 0 6% 40px 0;
  position: relative;
}
.recharge .limit-list ul li.active{
  border: 2px solid #f14500;
}
.recharge .limit-list ul li.active i{
  display: inline-block;
  width: 48px;
  height: 44px;
  background: url(../image/ok.png);
  position: absolute;
  left: 0;
  top: 0;
}
.recharge .limit-list ul li:nth-child(2n){margin-right: 0;}
.recharge .limit-list ul li .count-gold{
  font-size: 32px;
  color: #000;
}
.recharge .limit-list ul li .count-rmb{
  font-size: 26px;
  color: #999;
  margin-top: 2px;
}
.recharge .sub-btn{padding: 0 30px 40px;}
.recharge-manner{margin-top: 40px;}
.recharge-manner .btn-group{margin-bottom: 40px;}
.recharge-count{
  padding: 25px 28px 0 28px;
}
.recharge-count .form-group{
  height: 66px;
  line-height: 66px;
  border:none;
}
.recharge-count .control-label{
  width: 210px;
  font-size: 30px;
  color: #999;
}
.recharge-count .form-control{
  width: auto;
  font-size: 30px;
  height: auto;
  background: none;
}
.recharge-count .form-input{
  width: 152px;
  height: 55px;
  background: #fff;
  border:1px solid #afafaf;
  font-size: 48px;
  color: #000;
  padding-left: 5px;
  margin-right: 10px;
}
.recharge-count .btn-group{margin-top: 25px;}
.recharge-ok{
  padding: 30px;
}
.recharge-ok .title{
  text-align: center;
  font-size: 32px;
  color: #000;
}
.recharge-ok .title i{
  color: #27bd98;
  font-size: 48px;
  margin-right: 8px;
  vertical-align: -8px;
}
.recharge-ok .recharge-main{
  margin-top: 30px;
  background: #f8f8f8;
  border:1px solid #d2d2d2;
  border-radius: 5px;
  padding: 30px 70px 0;
}
.recharge-ok .recharge-main p{
  margin-bottom: 30px;
  color: #666;
}
/* =============================== 个人中心20141019 ========================== */
.personal-center{
  height: 100%;
  background: #f6f7f8;
  position: relative;
}
.personal-center .bd{
  margin-top: 68px;
  font-size: 32px;
}
.personal-center .bd .personal-message{
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  padding: 0 0 0 30px;
}
.personal-center .bd .personal-message li{
  padding: 28px 15px 28px 0;
  border-bottom: 1px solid #ccc;
  position: relative;
}
.personal-center .bd .personal-message li:last-child{border-bottom: none;}
.personal-center .bd .personal-message li:first-child{padding: 14px 15px 14px 0;}
.personal-center .bd .personal-message li:first-child .til{margin-top: 25px;}
.personal-center .bd .personal-message li .icon-m{margin-left: 20px;}
.personal-center .bd .personal-message li:first-child .icon-m{vertical-align: -25px;}
.personal-center .bd .personal-message li .til{color: #000; float: left;}
.personal-center .bd .personal-message li .mes-right{
  float: right;
  color: #999;
}
.personal-center .bd .personal-message li .mes-right img{
  width: 80px;
  height: 80px;
  border-radius: 80px;
}
.personal-center .footer-btn{
  position: absolute;
  bottom: 50px;
  width: 100%;
  padding: 15px;
}
.personal-center .footer-btn .out-btn{
  background: #e21313;
}
.personal-change-name{
  height: 100%;
  background: #f6f7f8;
}
.personal-change-name .bd{
  padding: 67px 15px;
}
.personal-change-name .change-name{
  width: 100%;
  height: 82px;
  line-height: 82px;
  background: #fff;
  border:1px solid #ccc;
  border-radius: 8px;
  margin-bottom: 60px;
}
.personal-change-name .change-name input{
  width: 100%;
  padding: 0 10px;
}
.personal-center .change-sex,
.personal-center .bind-phone{
  padding: 68px 15px 15px;
}
.personal-center .change-sex ul li{
  height: 90px;
  line-height: 90px;
  border:1px solid #ccc;
  margin-top: -1px;
  background: #fff;
  font-size: 32px;
  color: #000;
  padding: 0 20px;
}
.personal-center .change-sex ul li:first-child{
  border-radius: 6px 6px 0 0;
}
.personal-center .change-sex ul li:last-child{
  border-radius: 0 0 6px 6px;
}
.personal-center .change-sex ul li i{
  display: none;
  float: right;
  margin-top: 30px;
}
.personal-center .change-sex ul li.active i{display: inline-block;}
.personal-center .bind-phone{font-size: 32px;}
.personal-center .bind-phone .tip{margin:20px 0; font-size: 24px;}
.input-wrap{
  height: 93px;
  line-height: 90px;
  border:1px solid #ccc;
  background: #fff;
  border-radius: 6px;
  position: relative;
}
.input-wrap input{
  width: 100%;
  padding: 0 15px;
}
.personal-center .bind-phone .code-btn{
  position: absolute;
  padding: 20px;
  background: #34c42d;
  color: #fff;
  border-radius: 4px;
  right: 10px;
  top: 9px;
  line-height: 1;
}
/*================================= 设置2014-10-15 ===============================*/
.settings {
  padding-bottom: 73px;
  font-size: 32px;
  height: 650px;
  padding-top: 128px;
  overflow: auto;
}
.settings .title-s{
  font-size: 26px;
  padding: 28px 0 14px 31px;
  border-bottom: 1px solid #e5e5e5;
}
.settings ul li{
  background: #fff;
  border-bottom: 1px solid #e5e5e5;
  padding: 28px 15px 28px 30px;
}
.settings ul li.disable{
  cursor: default;
  color: #999;
}
.settings ul li .icon-m{color: #cacacf; font-size: 42px;}
.settings ul li .icon-m.active{color: #0d7aff;}
.settings ul li .icon-right{font-size: 28px; color: #999;}
.settings ul li .screen-size{margin-top: -10px;}
.settings ul li .screen-size a{
  display: inline-block;
  margin-left: 41px;
  padding: 12px;
  border: 2px solid #00afff;
  border-radius: 5px;
  color: #00afff;
}
.settings ul li .screen-size a.active{
  background: #00afff;
  color: #fff;
}
.settings ul li .preview-size{
  text-align: right;
  padding: 19px 0 0 15px;
  color: #fff;
  font-size: 24px;
}
.settings ul li .preview-size .preview-word{
  display: inline-block;
  background: #333;
  padding: 17px 40px;
  border-radius: 5px;
}
.settings ul li .location-screen{
  float: left;
  margin-right: 33px;
  text-align: center;
}
.settings ul li .location-screen:last-child{margin-right: 0;}
.settings ul li .location-screen .pic{
  width: 110px;
  height: 79px;
  border: 2px solid #00afff;
  margin-bottom: 6px;
}
.settings ul li .location-screen .pic .line-pic{
  width: 65px;
  height: 4px;
  background: #00afff;
  border-radius: 2px;
}
.settings ul li .location-screen .pic .line-1{margin:18px 0 0 7px;}
.settings ul li .location-screen .pic .line-2{margin:14px 0 0 21px;}
.settings ul li .location-screen .pic .line-3{margin:14px 0 0 33px;}
.settings ul li .location-screen .pic .line-4{margin:18px 0 0 20px;}
.settings ul li .location-screen .pic .line-5{margin:57px 0 0 20px;}
.settings ul li .location-screen span{
  font-size: 24px;
  color: #00afff;
}
.settings ul li .location-screen span.active{
  background: #00afff;
  color: #fff;
  padding: 3px 22px;
  border-radius: 20px;
}
.settings .sub-btn{
  padding: 50px 14px 0 14px;
}
.settings ul li .annotation{
  font-size: 24px;
  color: #999;
}
.settings .proportion-bg{
  width: 400px;
  height: 4px;
  background: #c3c3c3;
  position: relative;
  margin-top: 15px;
}
.settings .proportion-in{
  width: 50%;
  height: 4px;
  background: #00aefe;
}
.settings .proportion-dot{
  width: 38px;
  height: 38px;
  border:1px solid #ebebeb;
  border-radius: 19px;
  background: #fff;
  box-shadow: -1px 3px 8px 2px #ebebeb;
  position: absolute;
  left: 50%;
  top: -19px;
  margin-left: -19px;
}

/* ==================== 每日任务20141019 ========================== */
.task{
  height: 100%;
  background: #f6f7f8;
}
.task .bd{
  padding-top: 68px;
  padding-bottom: 40px;
}
.task-list{
  border-top:1px solid #d4d5d5;
  border-bottom:1px solid #d4d5d5;
}
.task-list li{
  height: 121px;
  position: relative;
  font-size: 24px;
  background: #fff;
}
.task-list li .main{
  margin-left: 30px;
  padding: 25px 0;
  border-bottom: 1px solid #d3d3d3;
}
.task-list li.active{
  background: #e5e5e5;
}
.task-list li.active .btn-task{
  background: #008ccc;
}
.task-list li .til{
  font-size: 32px;
  color: #000;
}
.task-list li .til .icon-m{
  margin-right: 10px;
  font-size: 32px;
}
.task-list li .tip{
  color: #999;
  margin-top: 14px;
}
.task-list li:last-child .main{border-bottom: none;}
.task-list li .btn-task{
  width: 100px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  color: #fff;
  background: #00afff;
  border-radius: 5px;
  position: absolute;
  right: 15px;
  top: 40px;
}
.task-list li.active .btn-finish,
.task-list li .btn-finish{
  background: #999;
}
/*====================== 每日一问/每日签到20141019 ====================*/
.day-box{
  height: 100%;
  background: #f6f7f8;
  font-size: 32px;
}
.day-box .bd{padding: 68px 15px 10px;}
.day-box .day-list {margin-bottom: 60px;}
.day-box .day-list li{

<body>
<div class="wrap">
  <!-- 订阅 -->
  <div class="subscribe">
    <!-- top顶部 -->
    <div class="top-bar">
      <div class="top-bar-bd">
        <p class="title">
          <i class="icon-m icon-left"></i>
          我的订阅
        </p>
      </div>
      <div class="top-bar-line"></div>
    </div>
    <!-- top顶部 -->

    <!-- 直播中模块 -->
    <div class="module">
      <div class="title clearfix">
        <a href="javascript:;" class="edit-btn">编辑订阅</a>
        <span class="title-name">直播中</span>
      </div>
      <div class="module-bd">
        <div class="video-list-wrap">
          <ul class="clearfix">
            <li>
              <img src="../image/pic01.jpg" alt="" class="game-pic">
              <div class="til">微笑19:00准时开播</div>
              <div class="detail clearfix">
                <span class="audience"><i class="icon-m icon-spectator"></i>8888</span>
                <p class="anchor">
                  <i class="icon-m icon-boy"></i>
                  <span>微笑</span>
                </p>
              </div>
            </li>
            <li>
              <img src="../image/pic01.jpg" alt="" class="game-pic">

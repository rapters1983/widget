<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta content="yes" name="apple-mobile-web-app-capable" />
  <meta content="black" name="apple-mobile-web-app-status-bar-style" />
  <meta content="telephone=no" name="format-detection" />
  <link rel="stylesheet" href="../css/reset.css">
  <link rel="stylesheet" href="../css/base.css">
  <link rel="stylesheet" href="../css/font-icon.css">
  <link rel="stylesheet" href="../css/api.css">
  <title>问题反馈</title>
</head>
<body>
<div class="wrap">
  <!-- 问题反馈 -->
  <div class="feedback">
    <!-- top顶部 -->
    <div class="top-bar">
      <div class="top-bar-bd">
        <p class="title">
          <i class="icon-m icon-left" id="btn-close"></i>
          问题反馈
        </p>
      </div>
      <div class="top-bar-line"></div>
    </div>
    <!-- top顶部 -->
    
    <div id="conWrap" class="scroll-wrap">
    <!-- 问题反馈 -->
      <div class="bd">
        
        <!-- 问题描述 -->
        <div class="group">
          <div class="til">问题描述</div>
          <div class="input-wrap textarea-wrap">
            <textarea name="contact" id="txt-desc" class="textarea-box" placeholder="请尽量详细描述问题或建议，为尽快解决问题请提供设备和网络信息。"></textarea>
          </div>
        </div>
        <!-- 问题描述 end-->

        <!-- 联系方式 -->
        <div class="group">
          <div class="til">联系方式</div>
          <div class="input-wrap">
            <input type="text" name="contact" id="txt-contact" placeholder="qq/手机/邮箱" autocapitalize="off" />
          </div>
        </div>
        <!-- 联系方式 end-->

        <div class="footer-btn">
          <div class="btn-group btn-block" id="btn-save">提交</div>
        </div>
      </div>
    <!-- 问题反馈 end -->
    </div>

  </div>
  <!-- 问题反馈 end-->
</div>
<script src="../script/lib/jquery-2.1.1.min.js"></script>
<script src="../script/api.js"></script>
<script src="../script/common.js"></script>
<script src="../script/feed-back.js"></script>
</body>
</html>
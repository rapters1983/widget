<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta content="yes" name="apple-mobile-web-app-capable" />
  <meta content="black" name="apple-mobile-web-app-status-bar-style" />
  <meta content="telephone=no" name="format-detection" />
  <link rel="stylesheet" href="../css/reset.css">
  <link rel="stylesheet" href="../css/base.css">
  <link rel="stylesheet" href="../css/font-icon.css">
  <title>充值成功</title>
</head>
<body>
<div class="wrap">
  <!-- 充值成功 -->
  <div class="recharge">
    <!-- top顶部 -->
    <div class="top-bar">
      <div class="top-bar-bd">
        <p class="title">
          <i class="icon-m icon-close" onclick="api.closeWin();"></i>
          金币充值
        </p>
      </div>
      <div class="top-bar-line"></div>
    </div>
    <!-- top顶部 -->
    
    <div id="conWrap" style="overflow:auto;">
      <!-- 充值成功 -->
      <div class="recharge-ok">
        <div class="title"><i class="icon-m icon-success"></i>充值成功</div>
        <div class="recharge-main">
          <p>充值帐号：<span class="org" id="txt-account"></span></p>
          <p>充值方式：<span class="c0" id="txt-payType"></span></p>
          <p>充值金额：<span class="org" id="txt-gold"></span><span class="c0">金币</span></p>
          <p>赠送金额：<span class="org" id="txt-coin"></span><span class="c0">战旗币</span></p>
        </div>
      </div>
      <!-- 充值成功 end-->
    </div>
  </div>
  <!-- 充值成功 end-->
</div>
<script src="../script/lib/jquery-2.1.1.min.js"></script>
<script src="../script/api.js"></script>
<script src="../script/common.js"></script>
<script src="../script/charge-ok.js"></script>
